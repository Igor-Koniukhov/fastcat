{{template "base" .}}
{{define "content"}}
    <div class="content">
        <div class="container">
            <div class="col-12">
                <form action="">
                    <div class="form-group p-2">
                        <label for="suppName">Select restaurant</label>
                        <select class="form-control" id="suppName" name="suppName">
                            {{range .Suppliers}}
                                <option>{{.Name}}</option>
                            {{end}}
                        </select>
                        <button type="submit" class="btn btn-primary mt-3">Choose</button>
                    </div>

                </form>
            </div>
            <div class="row">
                {{ range .Suppliers}}
                    <div class="col mb-3">
                        <div class="card  text-center p-2"
                             style="width: 18rem; height: 25rem; background-color: #ffc107e6">
                            <h3>{{.Name}}</h3>
                            <p><b>{{.Type}}</b></p>
                            <img src="{{.Image}}" style="background-color: black;" class="card-img-top"
                                 alt="{{.Name}} {{.Type}}.">
                            <div class="card-body">
                                <p>{{.WorkingHours.Opening}}</p>
                                <p>{{.WorkingHours.Closing}}</p>
                            </div>
                        </div>
                    </div>
                {{end}}

            </div>


            <div class="products-content grid-container">
